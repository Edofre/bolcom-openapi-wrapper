{
  "name": "edofre/bolcom-openapi-wrapper",
  "description": "Guzzle wrapper for the bol.com Open API",
  "keywords": [
    "bolcom",
    "openapi",
    "guzzle"
  ],
  "type": "library",
  "homepage": "https://github.com/edofre/bolcom-openapi-wrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Edofre\\BolCom\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Edofre\\BolCom\\Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5"
  },
  "scripts": {
    "test": "phpunit"
  }
}
